
<section class="module-register">
        <h2><span>个人信息设置<span></h2>
        <div class="set-info">
            <div class="set-item">
                <span>用户名：</span>
                <div class="input-box">
                    <input id="username" class="input" type="text" placeholder="必填" />
                </div>
            </div>
            <div class="set-item">
                <span>密码：</span>
                <div class="input-box">
                    <input id="password" class="input" type="password" placeholder="必填"/>
                </div>
            </div>
            <div class="set-item">
                <span>QQ：</span>
                <div class="input-box">
                    <input id="qq" class="input" type="text" />
                </div>
            </div>
            <div class="set-item">
                <span>手机：</span>
                <div class="input-box">
                    <input id="telphone" class="input" type="text" />
                </div>
            </div>
            <div class="set-item">
                <span>邮箱：</span>
                <div class="input-box">
                    <input id="email" class="input" type="text" placeholder="必填"/>
                </div>
            </div>
            <input  type="hidden" value="headPic/default.png" id="headPic"/>
                    
            <a href="javascript:;" class="save" node-type="register-submit">保存</a>
        </div>
</section>


